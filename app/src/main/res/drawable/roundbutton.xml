<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android" >

    <item >
        <shape android:shape="rectangle"  >
            <corners android:bottomRightRadius="12dp" android:bottomLeftRadius="12dp"
                android:topLeftRadius="12dp" android:topRightRadius="12dp"/>
            <stroke android:width="0.5dip" android:color="@color/baseColor" />
            <gradient
                android:angle="90"
                android:endColor="@color/baseColor"
                android:startColor="@color/baseColor"
                android:type="linear" />
        </shape>
    </item>
</selector>